        <activity
            android:name=".PriceOtherList"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LicensePlateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ServiceJournalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OilJournalActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".FuelActivityTest" />
        <activity android:name=".PriceAllActivity" />
        <activity
            android:name=".CusTomTestMainActivity"
            android:label="@string/title_activity_cus_tom_test_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SettingNewActivity"
            android:label="@string/title_activity_setting_new"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainNewActivity"
            android:label="@string/title_activity_main_new"
